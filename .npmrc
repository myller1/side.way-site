# Configurações para resolver problemas com dependências opcionais do Rollup
optional=false
fund=false
audit=false

# Força a instalação de dependências mesmo com problemas de plataforma
force=false
legacy-peer-deps=false

# Configurações específicas para Vercel
production=false
include=dev

# Evita problemas com dependências opcionais específicas de plataforma
ignore-optional=true